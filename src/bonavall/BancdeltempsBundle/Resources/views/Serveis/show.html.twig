{% extends 'bonavallBancdeltempsBundle:Default:skel.html.twig' %}

{% block body -%}
    <h1>Serveis</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Punts</th>
                <td>{{ entity.punts }}</td>
            </tr>
            <tr>
                <th>Descripcioservei</th>
                <td>{{ entity.descripcioServei }}</td>
            </tr>
            <tr>
                <th>Codipostal</th>
                <td>{{ entity.codiPostal }}</td>
            </tr>
            <tr>
                <th>Datainici</th>
                <td>{{ entity.dataInici|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Durada</th>
                <td>{{ entity.durada }}</td>
            </tr>
            <tr>
                <th>Datafinal</th>
                <td>{{ entity.dataFinal|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_serveis') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_serveis_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
