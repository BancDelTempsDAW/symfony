{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Usuari</h1>

    <table class="table-responsive">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Salt</th>
                <td>{{ entity.salt }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ entity.password }}</td>
            </tr>
            <tr>
                <th>Is Active</th>
                <td>{{ entity.isActive }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ entity.nom }}</td>
            </tr>
            <tr>
                <th>Cognom</th>
                <td>{{ entity.cognom }}</td>
            </tr>
            <tr>
                <th>Adreca</th>
                <td>{{ entity.adreca }}</td>
            </tr>
            <tr>
                <th>Telefon</th>
                <td>{{ entity.telefon }}</td>
            </tr>
            <tr>
                <th>Fotografia</th>
                <td>{{ entity.fotografia }}</td>
            </tr>
            <tr>
                <th>Presentacio</th>
                <td>{{ entity.presentacio }}</td>
            </tr>
            <tr>
                <th>Punts</th>
                <td>{{ entity.punts }}</td>
            </tr>
        </tbody>
    </table>

{% endblock %}
