{% extends '::base.html.twig' %}

{% block body -%}
    <h1>TipusServei</h1>

<div class="panel panel-default">
    <div class="container-fluid">
        <div class="table-responsive" >
    <table class="table table-condensed">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ entity.nom }}</td>
            </tr>
            <tr>
                <th>Descripcio</th>
                <td>{{ entity.descripcio }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_tipusservei') }}">
            Torna a la llista
        </a>
    </li>
    <li>
        <a href="{{ path('admin_tipusservei_edit', { 'id': entity.id }) }}">
            Edita
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
</div>
</div>
</div>        
{% endblock %}
