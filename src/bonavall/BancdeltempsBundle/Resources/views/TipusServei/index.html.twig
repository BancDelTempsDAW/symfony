{% extends '::base.html.twig' %}

{% block body -%}
    <h1>TipusServei list</h1>

<div class="panel panel-default">
    <div class="container-fluid">
        <div class="table-responsive" >
            <br />
            <table class="table table-condensed">
                <thead>
                <tr class="info">
                <th>Id</th>
                <th>Nom</th>
                <th>Descripcio</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('admin_tipusservei_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.nom }}</td>
                <td>{{ entity.descripcio }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('admin_tipusservei_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('admin_tipusservei_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('admin_tipusservei_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
  </div>
</div>
</div>
    {% endblock %}
